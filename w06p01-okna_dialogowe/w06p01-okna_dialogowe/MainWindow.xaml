<Window x:Class="w06p01_okna_dialogowe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:w06p01_okna_dialogowe"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="400">
    <StackPanel>
        <TextBlock x:Name="tb_01" FontSize="15pt"></TextBlock>
        <Button Click="Button_Click" >Okno odczytu z pliku</Button>
        <TextBlock x:Name="tb_02" FontSize="15pt"></TextBlock>
        <Button Click="Button_Click_1" >Okno zapisu do pliku</Button>
        <TextBlock x:Name="tb_03" FontSize="15pt"></TextBlock>
        <Button Click="Button_Click_2" >Własne okno dialogowe</Button>
    </StackPanel>
</Window>
