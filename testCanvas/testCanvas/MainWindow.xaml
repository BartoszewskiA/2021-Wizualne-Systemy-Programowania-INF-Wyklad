<Window x:Class="testCanvas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testCanvas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" TouchMove="Window_TouchMove" MouseMove="Window_MouseMove" >
    <Grid MouseMove="Grid_MouseMove" >
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Canvas x:Name="canvas" MouseMove="canvas_MouseMove">
            <Ellipse  x:Name="pilka" Height="100" Panel.ZIndex="20" Canvas.Left="176" Stroke="Black" Canvas.Top="106" Width="100"/>
           

        </Canvas>
        <TextBlock x:Name="opis" Grid.Row="1"></TextBlock>

    </Grid>
</Window>
